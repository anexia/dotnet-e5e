<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<RestorePackagesWithLockFile>true</RestorePackagesWithLockFile>
		<Authors>Anexia</Authors>
		<Description>A helper library to help you build functions on top of the Anexia Engine.</Description>
		<PackageLicenseUrl>https://github.com/anexia/dotnet-e5e/blob/main/LICENSE</PackageLicenseUrl>
		<RepositoryUrl>https://github.com/anexia/dotnet-e5e</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>anexia,faas</PackageTags>
		<DocumentationFile>bin\Release\net6.0\Anexia.E5E.xml</DocumentationFile>
		<TreatWarningsAsErrors>true</TreatWarningsAsErrors>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Hosting.Abstractions" Version="6.0.0"/>
		<PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="6.0.0"/>
	</ItemGroup>

</Project>
